<Page x:Class="ContentPage.ResultPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ContentPage"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="ResultPage">

    <TabControl Height="311" HorizontalAlignment="Left" Name="tabControl1" VerticalAlignment="Top" Width="503">
        <TabItem Header="Result" Name="ResultTable">
            <Grid>
                <DataGrid Height="278" HorizontalAlignment="Left" Margin="1,1,0,0" Name="dataGrid" VerticalAlignment="Top" Width="492" ItemsSource="{Binding DataList}">
                    <!--<DataGrid.Columns>
                        <DataGridTextColumn Width="100" Header="Key" Binding="{Binding}" />
                        <DataGridTextColumn Width="100" Header="Value" Binding="{Binding}" />
                        <DataGridTextColumn Width="100" Header="Integer" Binding="{Binding}" />
                    </DataGrid.Columns>-->
                </DataGrid>
            </Grid>
        </TabItem>
    </TabControl>

</Page>
